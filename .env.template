# Azure AI Foundry Demo Environment Variables
# Copy this file to .env and fill in your actual values

# Azure OpenAI (from your Azure AI Foundry deployment)
AZURE_OPENAI_ENDPOINT=https://your-foundry-endpoint.openai.azure.com/
AZURE_OPENAI_KEY=your-api-key-here
AZURE_OPENAI_DEPLOYMENT=your-deployment-name
AZURE_OPENAI_API_VERSION=2024-02-15-preview

# Azure AI Speech
AZURE_SPEECH_KEY=your-speech-key-here
AZURE_SPEECH_REGION=your-region-here

# Azure Translator
AZURE_TRANSLATOR_KEY=your-translator-key-here
AZURE_TRANSLATOR_REGION=your-region-here

# Azure Document Intelligence
AZURE_DOCINTEL_ENDPOINT=https://your-docintel-resource.cognitiveservices.azure.com/
AZURE_DOCINTEL_KEY=your-docintel-key-here
